    body:
      application/json:
        type: !include schemas/retrieveBiss/retrieve_uk_property_response.json
        examples:
          example-1:
            description: UK Property Summary
            value: !include examples/retrieveUkPropertyBiss/retrieve_response.json
    headers:
      X-CorrelationId:
        example: c75f40a6-a3df-4429-a697-471eeec46435
        description: Unique ID for operation tracking <br> String, 36 characters.
