{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Business Income Source Summary",
  "description": "Business Income Source Summary",
  "type": "object",
  "Properties":{
    "idType": {
      "type": "string",
      "description":"nino - national insurance number",
      "example":"nino"
    },
    "idValue": {
      "type": "string",
      "description": "nino - national insurance number",
      "example": "TC663795B"
    },
    "taxYear":{
      "type": "string",
      "description":"The tax year to which the data applies, in the format YYYY-YY",
      "example":  "2017-18"
    },
    "incomeSourceType":{
      "type": "string",
      "description": "Type of income source",
      "example": "self-employment"
    },
    "IncomeSourceId": {
      "type": "string",
      "description": "An identifier for the self-employment business, unique to the customer.",
      "example": "X9IS98470026982"
    }
  },
  "required": [
    "idType",
    "idValue",
    "taxYear",
    "incomeSourceType",
    "IncomeSourceId"
  ]
}